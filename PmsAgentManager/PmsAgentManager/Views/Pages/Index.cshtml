@model Dictionary<string, string>

<!DOCTYPE html>

<html lang="ru">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>Список подключений</title>
    <link href="~/css/styles.css" rel="stylesheet" asp-append-version="true" type="text/css" media="screen" runat="server"/>
</head>
<body>
<div>
    <h3 class="connection-title">Доступных подключений: @Model!.Count</h3>
    @if (Model.Count > 0)
    {
        <table class="connection-table">
            <thead>
            <th>Внешний код</th>
            <th>ID подключения</th>
            </thead>
            <tbody>
            @foreach (var connection in Model)
            {
                <tr>
                    <td>@connection.Value</td>
                    <td>@connection.Key</td>
                </tr>
            }
            </tbody>
        </table>
    }
</div>
</body>
</html>